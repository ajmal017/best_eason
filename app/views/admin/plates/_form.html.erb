<%= form_for [:admin, @plate] do |f| -%>
  <table class="grid_form">
    <tbody>
      <tr>
        <td class="field"><b></b><%= f.label :type %></td>
        <td><%= f.text_field :type, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :market %></td>
        <td><%= f.text_field :market, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :name %></td>
        <td><%= f.text_field :name, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :base_stock_id %></td>
        <td><%= f.number_field :base_stock_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :symbol %></td>
        <td><%= f.text_field :symbol, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :source_symbol %></td>
        <td><%= f.text_field :source_symbol, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :start_on %></td>
        <td><%= f.date_select :start_on, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :end_on %></td>
        <td><%= f.date_select :end_on, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :status %></td>
        <td><%= f.text_field :status, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :stocks_count %></td>
        <td><%= f.number_field :stocks_count, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :update_time %></td>
        <td><%= f.datetime_select :update_time, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"></td>
        <td><%= f.submit '提交' %></td>
      </tr>
    </tbody>
  </table>
  
<% end -%>


