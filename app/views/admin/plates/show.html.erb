<h1 class="title">
  <span class="title_left">Plate详情页面</span>
  <span class="title_right">
    <%= link_to "编辑", [:edit, :admin, @plate], style: "margin-right: 10px;" %>
    <%= link_to "返回列表", admin_plates_path %>
  </span>
</h1>

<table class="grid_form">
  <tbody>
    <tr>
      <td class="field">Id</td>
      <td>
        <%= @plate.id %>
      </td>
    </tr>
    <tr>
      <td class="field">Type</td>
      <td>
        <%= @plate.type %>
      </td>
    </tr>
    <tr>
      <td class="field">Market</td>
      <td>
        <%= @plate.market %>
      </td>
    </tr>
    <tr>
      <td class="field">Name</td>
      <td>
        <%= @plate.name %>
      </td>
    </tr>
    <tr>
      <td class="field">Base stock</td>
      <td>
        <%= @plate.base_stock_id %>
      </td>
    </tr>
    <tr>
      <td class="field">Symbol</td>
      <td>
        <%= @plate.symbol %>
      </td>
    </tr>
    <tr>
      <td class="field">Source symbol</td>
      <td>
        <%= @plate.source_symbol %>
      </td>
    </tr>
    <tr>
      <td class="field">Start on</td>
      <td>
        <%= @plate.start_on %>
      </td>
    </tr>
    <tr>
      <td class="field">End on</td>
      <td>
        <%= @plate.end_on %>
      </td>
    </tr>
    <tr>
      <td class="field">Status</td>
      <td>
        <%= @plate.status %>
      </td>
    </tr>
    <tr>
      <td class="field">Stocks count</td>
      <td>
        <%= @plate.stocks_count %>
      </td>
    </tr>
    <tr>
      <td class="field">Update time</td>
      <td>
        <%= @plate.update_time %>
      </td>
    </tr>
    <tr>
      <td class="field">Created at</td>
      <td>
        <%= @plate.created_at %>
      </td>
    </tr>
    <tr>
      <td class="field">Updated at</td>
      <td>
        <%= @plate.updated_at %>
      </td>
    </tr>
  </tbody>
</table>
