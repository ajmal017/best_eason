<h1 class="title">
  <span class="title_left"><%=@page_title%></span>
</h1>

<div class="box">
  <% Feedback::FEED_TYPES.each do |feed_type, type_desc| %>
    <%= link_to type_desc, admin_feedbacks_path(feed_type: feed_type), class: (params[:feed_type].to_s==feed_type.to_s ? "current" : nil) %>
  <% end %>
</div>

<%= page_info(@feedbacks) %>

<%= render partial: "list_#{params[:feed_type]}" %>

<%= will_paginate @feedbacks %>