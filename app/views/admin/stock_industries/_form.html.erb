<%= form_for [:admin, @stock_industry] do |f| -%>
  <table class="grid_form">
    <tbody>
      <tr>
        <td class="field"><b></b><%= f.label :base_stock_id, "财说内部Id" %></td>
        <td><%= f.number_field :base_stock_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :first_industry_code, "一级行业编码" %></td>
        <td><%= f.text_field :first_industry_code, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :first_industry_name, "一级行业名称" %></td>
        <td><%= f.text_field :first_industry_name, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :second_industry_code, "二级行业编码" %></td>
        <td><%= f.text_field :second_industry_code, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :second_industry_name, "二级行业名称" %></td>
        <td><%= f.text_field :second_industry_name, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :sector_code, "财说内部行业编码" %></td>
        <td><%= f.number_field :sector_code, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"></td>
        <td><%= f.submit '提交' %></td>
      </tr>
    </tbody>
  </table>
  
<% end -%>


