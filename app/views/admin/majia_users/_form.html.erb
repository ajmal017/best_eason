<%= form_for [:admin, @majia_user] do |f| -%>
  <table class="grid_form">
    <tbody>
      <tr>
        <td class="field"><b></b><%= f.label :email %></td>
        <td><%= f.text_field :email, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :password %></td>
        <td><%= f.text_field :password, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :username %></td>
        <td><%= f.text_field :username, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :gender %></td>
        <td><%= f.check_box :gender, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :province %></td>
        <td><%= f.text_field :province, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :city %></td>
        <td><%= f.number_field :city, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :headline %></td>
        <td><%= f.text_field :headline, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :orientation %></td>
        <td><%= f.text_field :orientation, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :concern %></td>
        <td><%= f.text_field :concern, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :duration %></td>
        <td><%= f.text_field :duration, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"></td>
        <td><%= f.submit '提交' %></td>
      </tr>
    </tbody>
  </table>
  
<% end -%>


