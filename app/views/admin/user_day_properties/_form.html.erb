<%= form_for [:admin, @user_day_property] do |f| -%>
  <table class="grid_form">
    <tbody>
      <tr>
        <td class="field"><b>*</b><%= f.label :user_id %></td>
        <td><%= f.number_field :user_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b>*</b><%= f.label :date %></td>
        <td><%= f.date_select :date, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :total %></td>
        <td><%= f.text_field :total, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :total_cash %></td>
        <td><%= f.text_field :total_cash, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :total_stocks_cost %></td>
        <td><%= f.text_field :total_stocks_cost, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :base_currency %></td>
        <td><%= f.text_field :base_currency, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :trading_account_id %></td>
        <td><%= f.number_field :trading_account_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"></td>
        <td><%= f.submit '提交' %></td>
      </tr>
    </tbody>
  </table>
  
<% end -%>


