<%= form_for [:admin, @admin_log] do |f| -%>
  <table class="grid_form">
    <tbody>
      <tr>
        <td class="field"><b></b><%= f.label :staffer_id %></td>
        <td><%= f.number_field :staffer_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :content %></td>
        <td><%= f.text_field :content, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :log_type %></td>
        <td><%= f.text_field :log_type, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :request_ip %></td>
        <td><%= f.text_field :request_ip, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"></td>
        <td><%= f.submit '提交' %></td>
      </tr>
    </tbody>
  </table>
  
<% end -%>


