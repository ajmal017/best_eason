<%= form_for [:admin, @contest] do |f| -%>
  <table class="grid_form">
    <tbody>
      <tr>
        <td class="field"><b></b><%= f.label :status %></td>
        <td><%= f.select :status, f.object.class.status_options %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :name %></td>
        <td><%= f.text_field :name, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :start_at %></td>
        <td><%= f.datetime_select :start_at, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :end_at %></td>
        <td><%= f.datetime_select :end_at, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :broker_id %></td>
        <td><%= f.collection_select :broker_id, Broker.all, :id, :cname, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :users_count %></td>
        <td><%= f.number_field :users_count, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :total_invest %></td>
        <td><%= f.text_field :total_invest, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"></td>
        <td><%= f.submit '提交' %></td>
      </tr>
    </tbody>
  </table>
  
<% end -%>


