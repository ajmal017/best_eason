<%= form_for [:admin, @historical_quote_retry] do |f| -%>
  <table class="grid_form">
    <tbody>
      <tr>
        <td class="field"><b></b><%= f.label :base_stock_id %></td>
        <td><%= f.number_field :base_stock_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :date %></td>
        <td><%= f.date_select :date, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :trading_date %></td>
        <td><%= f.date_select :trading_date, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :number %></td>
        <td><%= f.number_field :number, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"></td>
        <td><%= f.submit '提交' %></td>
      </tr>
    </tbody>
  </table>
  
<% end -%>


