<%= form_for [:admin, @plate_stock] do |f| -%>
  <table class="grid_form">
    <tbody>
      <tr>
        <td class="field"><b>*</b><%= f.label :plate_id %></td>
        <td><%= f.number_field :plate_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b>*</b><%= f.label :base_stock_id %></td>
        <td><%= f.number_field :base_stock_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :weight %></td>
        <td><%= f.text_field :weight, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :start_on %></td>
        <td><%= f.date_select :start_on, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :end_on %></td>
        <td><%= f.date_select :end_on, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :status %></td>
        <td><%= f.text_field :status, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"></td>
        <td><%= f.submit '提交' %></td>
      </tr>
    </tbody>
  </table>
  
<% end -%>


