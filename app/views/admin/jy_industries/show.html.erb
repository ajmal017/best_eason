<h1 class="title">
  <span class="title_left">Jy industry详情页面</span>
  <span class="title_right">
    <%= link_to "编辑", [:edit, :admin, @jy_industry], style: "margin-right: 10px;" %>
    <%= link_to "返回列表", admin_jy_industries_path %>
  </span>
</h1>

<table class="grid_form">
  <tbody>
    <tr>
      <td class="field">Id</td>
      <td>
        <%= @jy_industry.id %>
      </td>
    </tr>
    <tr>
      <td class="field">Name</td>
      <td>
        <%= @jy_industry.name %>
      </td>
    </tr>
    <tr>
      <td class="field">Code</td>
      <td>
        <%= @jy_industry.code %>
      </td>
    </tr>
    <tr>
      <td class="field">Level</td>
      <td>
        <%= @jy_industry.level %>
      </td>
    </tr>
    <tr>
      <td class="field">Created at</td>
      <td>
        <%= @jy_industry.created_at %>
      </td>
    </tr>
    <tr>
      <td class="field">Updated at</td>
      <td>
        <%= @jy_industry.updated_at %>
      </td>
    </tr>
  </tbody>
</table>
