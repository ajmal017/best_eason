<%= form_for [:admin, @stock_component] do |f| -%>
  <table class="grid_form">
    <tbody>
      <tr>
        <td class="field"><b></b><%= f.label :base_stock_id %></td>
        <td><%= f.number_field :base_stock_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :inner_code %></td>
        <td><%= f.number_field :inner_code, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :cs_code %></td>
        <td><%= f.number_field :cs_code, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :name %></td>
        <td><%= f.text_field :name, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"></td>
        <td><%= f.submit '提交' %></td>
      </tr>
    </tbody>
  </table>
  
<% end -%>


