<%= form_for [:admin, @ib_id_change_log] do |f| -%>
  <table class="grid_form">
    <tbody>
      <tr>
        <td class="field"><b></b><%= f.label :symbol %></td>
        <td><%= f.text_field :symbol, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :ib_id %></td>
        <td><%= f.text_field :ib_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :date %></td>
        <td><%= f.date_select :date, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"></td>
        <td><%= f.submit '提交' %></td>
      </tr>
    </tbody>
  </table>
  
<% end -%>


