<%= form_for [:admin, @account_value_archive] do |f| -%>
  <table class="grid_form">
    <tbody>
      <tr>
        <td class="field"><b></b><%= f.label :user_id %></td>
        <td><%= f.number_field :user_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :broker_user_id %></td>
        <td><%= f.text_field :broker_user_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :key %></td>
        <td><%= f.text_field :key, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :currency %></td>
        <td><%= f.text_field :currency, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :value %></td>
        <td><%= f.text_field :value, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :user_binding_id %></td>
        <td><%= f.number_field :user_binding_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :archive_date %></td>
        <td><%= f.date_select :archive_date, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :base_currency %></td>
        <td><%= f.text_field :base_currency, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :trading_account_id %></td>
        <td><%= f.number_field :trading_account_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"></td>
        <td><%= f.submit '提交' %></td>
      </tr>
    </tbody>
  </table>
  
<% end -%>


