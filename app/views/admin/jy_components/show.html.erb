<h1 class="title">
  <span class="title_left">Jy component详情页面</span>
  <span class="title_right">
    <%= link_to "编辑", [:edit, :admin, @jy_component], style: "margin-right: 10px;" %>
    <%= link_to "返回列表", admin_jy_components_path %>
  </span>
</h1>

<table class="grid_form">
  <tbody>
    <tr>
      <td class="field">Id</td>
      <td>
        <%= @jy_component.id %>
      </td>
    </tr>
    <tr>
      <td class="field">Begin date</td>
      <td>
        <%= @jy_component.begin_date %>
      </td>
    </tr>
    <tr>
      <td class="field">L one name</td>
      <td>
        <%= @jy_component.l_one_name %>
      </td>
    </tr>
    <tr>
      <td class="field">L one code</td>
      <td>
        <%= @jy_component.l_one_code %>
      </td>
    </tr>
    <tr>
      <td class="field">L two name</td>
      <td>
        <%= @jy_component.l_two_name %>
      </td>
    </tr>
    <tr>
      <td class="field">L two code</td>
      <td>
        <%= @jy_component.l_two_code %>
      </td>
    </tr>
    <tr>
      <td class="field">Flag</td>
      <td>
        <%= @jy_component.flag %>
      </td>
    </tr>
    <tr>
      <td class="field">Created at</td>
      <td>
        <%= @jy_component.created_at %>
      </td>
    </tr>
    <tr>
      <td class="field">Updated at</td>
      <td>
        <%= @jy_component.updated_at %>
      </td>
    </tr>
  </tbody>
</table>
