<!--页面标题-->
<h1 class="title">
 <span class="title_left"><%=@page_title%></span>
</h1>

<%= form_tag admin_weekly_reports_path do %>
  <table class="grid_form">
    <tbody>
      <tr>
        <td class="field"><b>*</b>标题:</td>
        <td>
          <%= text_field_tag :title %><br/>
        </td>
      </tr>
      <tr>
        <td class="field"><b>*</b>URL:</td>
        <td>
          <%= text_field_tag :url %><br/>
          必须http格式  http://
        </td>
      </tr>
      <tr>
        <td class="field"></td>
        <td>
          <%= submit_tag "创建" %>
        </td>
      </tr>
    </tbody>
  </table>
<% end -%>



