<h1 class="title">
  <span class="title_left">Source详情页面</span>
  <span class="title_right">
    <%= link_to "编辑", [:edit, :admin, @es_source], style: "margin-right: 10px;" %>
    <%= link_to "返回列表", admin_es_sources_path %>
  </span>
</h1>

<table class="grid_form">
  <tbody>
    <tr>
      <td class="field">Id</td>
      <td>
        <%= @es_source.id %>
      </td>
    </tr>
    <tr>
      <td class="field">Code</td>
      <td>
        <%= @es_source.code %>
      </td>
    </tr>
    <tr>
      <td class="field">Name</td>
      <td>
        <%= @es_source.name %>
      </td>
    </tr>
    <tr>
      <td class="field">Url</td>
      <td>
        <%= @es_source.url %>
      </td>
    </tr>
    <tr>
      <td class="field">Created at</td>
      <td>
        <%= @es_source.created_at %>
      </td>
    </tr>
    <tr>
      <td class="field">Updated at</td>
      <td>
        <%= @es_source.updated_at %>
      </td>
    </tr>
  </tbody>
</table>
