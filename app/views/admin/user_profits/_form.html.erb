<%= form_for [:admin, @user_profit] do |f| -%>
  <table class="grid_form">
    <tbody>
      <tr>
        <td class="field"><b></b><%= f.label :user_id %></td>
        <td><%= f.number_field :user_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :today_pnl %></td>
        <td><%= f.text_field :today_pnl, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :total_pnl %></td>
        <td><%= f.text_field :total_pnl, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :date %></td>
        <td><%= f.date_select :date, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"><b></b><%= f.label :trading_account_id %></td>
        <td><%= f.number_field :trading_account_id, size: 30 %></td>
      </tr>
      <tr>
        <td class="field"></td>
        <td><%= f.submit '提交' %></td>
      </tr>
    </tbody>
  </table>
  
<% end -%>


