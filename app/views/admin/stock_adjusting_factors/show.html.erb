<h1 class="title">
  <span class="title_left">Stock adjusting factor详情页面</span>
  <span class="title_right">
    <%= link_to "编辑", [:edit, :admin, @stock_adjusting_factor], style: "margin-right: 10px;" %>
    <%= link_to "返回列表", admin_stock_adjusting_factors_path %>
  </span>
</h1>

<table class="grid_form">
  <tbody>
    <tr>
      <td class="field">Id</td>
      <td>
        <%= @stock_adjusting_factor.id %>
      </td>
    </tr>
    <tr>
      <td class="field">Base stock</td>
      <td>
        <%= @stock_adjusting_factor.base_stock_id %>
      </td>
    </tr>
    <tr>
      <td class="field">Inner code</td>
      <td>
        <%= @stock_adjusting_factor.inner_code %>
      </td>
    </tr>
    <tr>
      <td class="field">Ex divi date</td>
      <td>
        <%= @stock_adjusting_factor.ex_divi_date %>
      </td>
    </tr>
    <tr>
      <td class="field">Adjusting factor</td>
      <td>
        <%= @stock_adjusting_factor.adjusting_factor %>
      </td>
    </tr>
    <tr>
      <td class="field">Adjusting const</td>
      <td>
        <%= @stock_adjusting_factor.adjusting_const %>
      </td>
    </tr>
    <tr>
      <td class="field">Ratio adjusting factor</td>
      <td>
        <%= @stock_adjusting_factor.ratio_adjusting_factor %>
      </td>
    </tr>
    <tr>
      <td class="field">Xgrq</td>
      <td>
        <%= @stock_adjusting_factor.xgrq %>
      </td>
    </tr>
    <tr>
      <td class="field">Accu cash divi</td>
      <td>
        <%= @stock_adjusting_factor.accu_cash_divi %>
      </td>
    </tr>
    <tr>
      <td class="field">Accu bonus share ratio</td>
      <td>
        <%= @stock_adjusting_factor.accu_bonus_share_ratio %>
      </td>
    </tr>
    <tr>
      <td class="field">Created at</td>
      <td>
        <%= @stock_adjusting_factor.created_at %>
      </td>
    </tr>
    <tr>
      <td class="field">Updated at</td>
      <td>
        <%= @stock_adjusting_factor.updated_at %>
      </td>
    </tr>
  </tbody>
</table>
