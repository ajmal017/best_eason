$(".normal").hide();
$(".attended").hide();
$(".alert").hide();


<% if @survey.valid? %>
  $(".bomb_box").show();
  $(".normal").show();
<% else %>
   <% if !@survey.q1_12 || !@survey.at_least_one || @survey.q4.blank? || @survey.q5.blank? || @survey.user_name.blank? || @survey.user_phone.blank? %>
     $(".alert").show();
   <% else %>
     $(".bomb_box").show();
     $(".attended").show();
   <% end %>
<% end %>
